public class LinkedList
{
	Node head;
	
	static class Node 
	{
		int value;
		Node next;
		
		Node(int d)
		{
			value=d;
			next=null;
		}
	}
	
	Node n=head;
	void print()
	{
		while(n!=null)
		{
			System.out.println(n.value);
			n=n.next;
		}
		
	}
	
	public static void main(String[] s)
	{
		LinkedList llist = new LinkedList();
		
		llist.head= new Node(1);
		Node second = new Node(2);
		Node third = new Node(3);
		
		llist.head.next = second;
		second.next = third;
		
		llist.print();
		
	}
	
}
